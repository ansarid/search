{
  "manifest_version": 2,
  "name": "Google",
  "description": "Search Engine for Google",
  "version": "1.0",
  "icons": {
      "32": "icons/32.png",
      "48": "icons/48.png",
      "64": "icons/64.png",
      "96": "icons/96.png",
      "128": "icons/128.png",
      "144": "icons/144.png",
      "256": "icons/256.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "google@search-engine.tombulled",
      "strict_min_version": "55.0"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Google",
      "search_url": "https://www.google.co.uk/search?q={searchTerms}",
      "keyword": "g",
      "favicon_url": "https://www.google.co.uk/images/branding/googleg/1x/googleg_standard_color_64dp.png",
      "is_default": false,
      "encoding": "UTF-8"
    }
  }
}
