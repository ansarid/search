{
  "manifest_version": 2,
  "name": "London South East",
  "description": "Search Engine for London South East",
  "version": "1.0",
  "icons": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "64": "icons/64.png",
      "96": "icons/96.png",
      "128": "icons/128.png",
      "256": "icons/256.png",
      "512": "icons/16.png",
      "1024": "icons/16.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "london-south-east@search-engine.tombulled",
      "strict_min_version": "55.0"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "London South East",
      "search_url": "https://www.lse.co.uk/share-prices/company-lookup.html?search={searchTerms}",
      "keyword": "lse",
      "favicon_url": "https://static.lse.co.uk/images/favicon.ico",
      "is_default": false,
      "encoding": "UTF-8"
    }
  }
}
