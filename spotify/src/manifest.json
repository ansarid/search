{
  "manifest_version": 2,
  "name": "Spotify",
  "description": "Search Engine for Spotify",
  "version": "1.0",
  "icons": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "96": "icons/96.png",
      "128": "icons/128.png",
      "196": "icons/196.png",
      "256": "icons/256.png",
      "512": "icons/512.png",
      "1024": "icons/1024.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "spotify@search-engine.tombulled",
      "strict_min_version": "55.0"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Spotify",
      "search_url": "https://open.spotify.com/search/{searchTerms}",
      "keyword": "sp",
      "favicon_url": "https://open.scdn.co/cdn/images/favicon.5cb2bd30.ico",
      "is_default": false,
      "encoding": "UTF-8"
    }
  }
}
